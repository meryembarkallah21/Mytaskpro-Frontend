        "build:test-api": "ng build --project plugins --prod --modulePath=./test-api/test-api.module#TestApiModule --pluginName=test-api --sharedLibs=shared  --outputPath=./src/assets/plugins",
        "build:plugins": "npm run build:shared && npm run build:plugin1 && npm run build:plugin2 && npm run build:test-api"
    },
